*{
	box-sizing: border-box;
}
body{
	background: url("spaceShips.jpg");
	font-family: Ubuntu;
}
table{
	width: 620px;
	margin: 10px auto;
}
button{
	border: 2px solid #fff;
	background: #000;
	box-shadow:0 0 20px #555;
	color: #fff;
	width: 70px;
	height: 50px;
	transition: 0.5s;
	font-weight: bold;
	border-radius: 20px;
}
button:hover{
	border: 2px solid #fa5a5a;
	transform: scale(1.05);
	transition: 0.5s;
	text-align: center;
}
a{
	text-decoration: none;
	color: #fff;
}
.button{
	width: 5%;
	margin: 0 auto;
}
.symbols{
	color: rgba(255, 255, 255, 1);
	font-size: 20px;
	text-shadow: 2px 1px 1px #000;
	font-family: Josefin Sans;
}
.symbols td{
	padding: 10px;
	text-align: center;
}
.space td{
	padding: 15px;
}
.head{
	color: #fff;
	text-align: center;
	margin-top: 20px;
}
.head h1{
	background-color: #000;
	width: 15%;
	padding: 10px;
	margin: 0 auto;
	border: 2px solid #fff;
	border-radius: 30px;
	transition: 1s;
}
.head h1:hover{
	transform: scale(1.1);
	transition: 1s;
}
@media (max-width: 1000px){
	body{
		background: url(backgroundPhone.jpg);
		background-size: 140%;
		background-position: center;
	}
	.head{
		margin-top: 50%;
	}
	table{
		margin: 100px auto;
	}
}